    "classic",
    "retail",
    "starcraft",
    "sc2",
    "diablo",
    "d3",
    "hs",
    "hearthstone",
    "ow",
    "overwatch"
  ],
  "dependencies": {},
  "peerDependencies": {
    "@blizzard-api/core": "0.1.1"
  },
  "devDependencies": {
    "@blizzard-api/core": "workspace:*"
  },
  "scripts": {
    "build": "tsup",
    "debv": "tsup --watch",
    "test": "vitest run",
    "test:coverage": "pnpm test -- --coverage",
    "test:watch": "vitest watch"
  }
