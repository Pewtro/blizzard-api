{
  "name": "@blizzard-api/wow",
  "version": "0.2.3",
  "license": "MIT",
  "author": "Putro",
  "description": "A series of helpers to interact with the World of Warcraft Blizzard API",
  "repository": "https://github.com/Pewtro/blizzard/tree/main/packages/wow",
  "type": "module",
